<refentry xml:id="zmqsocket.ispersistent" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ZMQSocket::isPersistent</refname>
  <refpurpose>Whether the socket is persistent</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>boolean</type><methodname>ZMQSocket::isPersistent</methodname>
   <void />
  </methodsynopsis>
  <para>
   Check whether the socket is persistent.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns a &boolean; based on whether the socket is persistent or not.
  </para>
 </refsect1>


</refentry>

